{
  "productName": "Voice Note AI",
  "version": "1.0.0",
  "identifier": "com.voicenoteai.app",
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:3000",
    "frontendDist": "../out"
  },
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Voice Note AI",
        "width": 800,
        "height": 600,
        "minWidth": 400,
        "minHeight": 300,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "iOS": {
      "developmentTeam": ""
    },
    "android": {
      "minSdkVersion": 24,
      "targetSdkVersion": 34
    }
  },
  "plugins": {
    "shell": {
      "open": true
    }
  }
}
